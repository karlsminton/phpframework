{
  "head": {
    "element": true,
    "children": [
      {
        "title": {
          "element": true,
          "value": "Phpframework Example Site"
        }
      }
    ]
  },
  "body": {
    "element": true,
    "children": [
      {
        "header": {
          "element": true,
          "children": [{
            "logo": {
              "viewmodel": "Phpframework\\Core\\View\\Model\\Logo",
              "template": "logo.phtml"
            },
            "menu": {
              "viewmodel": "Phpframework\\Core\\View\\Model\\Menu",
              "template": "menu.phtml"
            }
          }]
        },
        "main": {
          "element": true,
          "children": [{
            "content": {
              "viewmodel": "Phpframework\\Core\\View\\Model\\NotFound",
              "template": "notfound.phtml"
            }
          }]
        },
        "footer": {
          "element": true,
          "children": [{
            "copyright": {
              "viewmodel": "Phpframework\\Core\\View\\Model\\Copyright",
              "template": "copyright.phtml"
            }
          }]
        }
      }
    ]
  }
}